<html lang='en'>

  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <meta http-equiv='X-UA-Compatible' content='ie=edge' />
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/css/bootstrap.min.css'
    />
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0-11/css/all.min.css'
    />
    <style>
      *{ font-size: 16px; } table { margin: 0 auto; width: 60%; } .container {
      max-width: 100%; } .content { display: block; margin: 0 auto; font-family:
      Arial, Helvetica, sans-serif; align-items: center; text-align: left;
      max-width: 100%; } .ref { font-weight: 600; } @media only screen and
      (min-width: 320px) and (max-width: 425px) { .and_AC { padding: 0; }
      .and_Add { padding-left: 0; } .iOS_AC { padding: 20px; word-break:
      break-all; } .QR { display: flex; flex-direction: column; } .instruction {
      word-break: break-all; } }
    </style>

  </head>

  <body>
    <div class='container' style='flex-direction: column;'>
      <div class='content'>
        <table class='table'>

          <tbody>
            <tr>
              <td class='ref'>Order Number:</td>
              <td class='left_Td'>{{order_id}}</td>
            </tr>
            <tr>
              <td class='ref'>Order Date:</td>
              <td class='left_Td'>{{order_date}}</td>
            </tr>
            <tr>
              <td class='ref'>ICCID:</td>
              <td class='left_Td'>{{iccid}}</td>
            </tr>
            <tr>
              <td class='ref'>APN (Access Point Name):</td>
              <td class='left_Td'>{{apn}}</td>
            </tr>
            <tr>
              <td class='ref'>Data Roaming:</td>
              <td class='left_Td'>{{dataRoaming}}</td>
            </tr>
            {{!-- <tr>
            <td class="ref">Issue to:</td>
            <td class="left_Td">{{email}}
            </td>
          </tr> --}}
            <tr>
              <td class='ref'>Package Purchase:</td>
              <td class='left_Td'>{{planName}}
                {{packageData}}
                GB -
                {{packageValidity}}
                Days</td>
            </tr>
          </tbody>
        </table>
        <br />
        <table class='table'>
          <h5 style='text-align: center; font-size: 20px;'>Manual Installation
            for Android:</h5>
          <tbody>
            <tr>
              <td class='ref'>SM - DP + Address:</td>
              <td class='and_Add'>{{iosAddress}}</td>
            </tr>
            <tr>
              <td class='ref'>Activation Code:</td>
              <td class='and_AC'>{{iosURL}}</td>
            </tr>
          </tbody>
        </table>
        <table class='table'>
          <h5 style='text-align: center; font-size: 20px;'>Manual Installation
            for IOS:</h5>
          <tr>
            <td class='ref'>Activation Code:</td>
            <td class='iOS_AC'>{{qrCodeString}}</td>
          </tr>
        </table>
        <br />
        <div class='QR text-center'>
          <p style='text-align: center;'>QR CODE:</p>
          <img
            style='position: relative; left: 50%; transform: translateX(-50%);'
            src={{qr_url}}
            alt='QR_Scan'
          />
        </div>
      </div>
      <div class='instruction'>
        <h5 style='font-size: 20px;'>Setup Instructions for your new eSIM</h5>
        <div>
          <p>Android User:
